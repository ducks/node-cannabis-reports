            body:
              application/json:
                example: !include raml/data/flowers/flowers-user-200.js
          404:
            body:
              application/json:
                example: !include raml/data/flowers/flowers-404.js

    /reviews:
      get:
        description: Get the reviews for a cannabis flower.
        queryParameters:
